{
  "name": "less-openui5",
  "version": "0.8.7",
  "description": "Build OpenUI5 themes with Less.js",
  "scripts": {
    "lint": "eslint lib test",
    "unit": "mocha test/*.js",
    "unit-debug": "mocha --inspect --inspect-brk test/*.js",
    "test": "npm run lint && npm run unit",
    "preversion": "npm test",
    "version": "git-chglog --next-tag v$npm_package_version -o CHANGELOG.md 0.7.0.. && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "release-note": "git-chglog -c .chglog/release-config.yml v$npm_package_version"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/less-openui5.git"
  },
  "files": [
    "CONTRIBUTING.md",
    "lib/**",
    "LICENSES/**",
    ".reuse/**"
  ],
  "keywords": [
    "less",
    "less.js",
    "openui5",
    "theme"
  ],
  "author": "SAP SE",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 10"
  },
  "dependencies": {
    "clone": "^2.1.0",
    "css": "^2.2.1",
    "mime": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "graceful-fs": "^4.2.4",
    "mocha": "^7.2.0"
  }
}
